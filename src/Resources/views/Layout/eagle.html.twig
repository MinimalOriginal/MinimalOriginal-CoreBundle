{% extends "base.html.twig" %}
{%- block body %}
  {%- block content %}
    <div class="expanded row">
      <div class="medium-3 large-2 medium-pull-9 large-pull-10 columns" data-sticky-container>
        <div class="sticky" data-sticky data-margin-top="0">{%- block sidebar %}{% endblock -%}</div>
      </div>
      <div class="small-12 medium-9 large-10 medium-push-3 large-push-2 columns">{%- block frame %}{%- block data %}{% endblock -%}{% endblock -%}</div>
    </div>
  {% endblock -%}
{% endblock -%}
